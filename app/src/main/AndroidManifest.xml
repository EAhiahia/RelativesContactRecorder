<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.limboooo.contactrecorder"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".activity.ProjectApplication"
        android:allowBackup="true"
        android:icon="@drawable/my_launcher_icon"
        android:roundIcon="@drawable/my_launcher_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.RelativesContactRecorder"
        tools:targetApi="31">
        <activity
            android:name=".activity.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.RelativesContactRecorder">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>